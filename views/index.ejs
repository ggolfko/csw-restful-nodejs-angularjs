    <!-- Create Modal -->
    <div id="create-modal" class="modal-demo">
      <button type="button" class="close" onclick="Custombox.close();">
      <span>&times;</span><span class="sr-only">Close</span>
  </button>
      <h4 class="custom-modal-title">Add Contact</h4>
      <div class="custom-modal-text text-left">
        <form role="form" ng-submit="add($event)">
          <div class="form-group" ng-class="{'has-error': error.picUrl}">
            <label for="picUrl">PicUrl</label>
            <input type="text" class="form-control" placeholder="Enter picurl" ng-model="addCrm.picUrl">
            <span class="help-block" ng-show="error.picUrl">
                            You picture url is required.
                        </span>
          </div>
          <div class="form-group" ng-class="{'has-error': error.name}">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" placeholder="Enter name" ng-model="addCrm.name" ng-init="addCrm.name=''">
            <span class="help-block" ng-show="error.name">
                            You name is required.
                        </span>
          </div>
          <div class="form-group" ng-class="{'has-error': error.username}">
            <label for="name">Username</label>
            <input type="text" class="form-control" id="name" placeholder="Enter username" ng-model="addCrm.username" ng-init="addCrm.username=''">
            <span class="help-block" ng-show="error.name">
                            You username is required.
                        </span>
          </div>
          <div class="form-group" ng-class="{'has-error': error.email}">
            <label for="exampleInputEmail1">Email address</label>
            <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email" ng-model="addCrm.email" ng-init="addCrm.email=''">
            <span class="help-block" ng-show="error.email">
                            You email is required.
                        </span>
          </div>
          <div class="form-group" ng-class="">
            <label for="position">Phone</label>
            <input type="text" class="form-control" id="position" placeholder="Enter phone" ng-model="addCrm.phone">
          </div>
          <h4 class="m-b-20 header-title"><b>Address</b></h4>
      <div class="form-group" ng-class="">
        <input type="text" class="form-control" id="company" placeholder="Enter latitude" ng-model="addCrm.lat">
      </div>
      <div class="form-group" ng-class="">
        <input type="text" class="form-control" id="company" placeholder="Enter longitude" ng-model="addCrm.lng">
      </div>
      <h4 class="m-b-20 header-title"><b>Company</b></h4>
      <div class="form-group" ng-class="">
        <label for="company">Company</label>
        <input type="text" class="form-control" id="company" placeholder="Enter company" ng-model="addCrm.companyname">
      </div>

      <button type="submit" ng-disabled="data.executed" class="btn btn-default waves-effect waves-light">Save</button>
      <button type="button" onclick="Custombox.close();" class="btn btn-danger waves-effect waves-light m-l-10">Cancel</button>
      </form>
    </div>
  </div>

