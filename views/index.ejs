                            <span class="help-block" ng-show="error.picUrl">
                                            You picture url is required.
                                        </span>
                          </div>
                        </div>
                        <div class="form-group" ng-class="{'has-error': error.name}">
                          <label class="col-md-2 control-label">Name</label>
                          <div class="col-md-10">
                            <input type="text" class="form-control" ng-model="editCrm.name">
                            <span class="help-block" ng-show="error.name">
                                            You name is required.
                                        </span>
                          </div>
                        </div>
                        <div class="form-group" ng-class="{'has-error': error.username}">
                          <label class="col-md-2 control-label">Username</label>
                          <div class="col-md-10">
                            <input type="text" class="form-control" ng-model="editCrm.username">
                            <span class="help-block" ng-show="error.username">
