                            <input type="text" class="form-control" ng-model="editCrm.bs">
                            <span class="help-block" ng-show="error.bs">
                                            You bs is required.
                                        </span>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>


        </div>
        <!-- container -->

      </div>
      <!-- content -->

      <footer class="footer text-right">
        2017 Â© client-server with crm.
